package kr.or.ddit.ibatis.vo.memberVO;

import java.io.Serializable;

/**
 * 출석관리
 * 
 * @author sanghoyun
 *
 */
public class AttendanceVO implements Serializable{
	private String mem_id;
	private String days_check;
	private String days_date;
	private int count;
	private String attendance_date;
	private int attendance_count;

	public String getAttendance_date() {
		return attendance_date;
	}

	public void setAttendance_date(String attendance_date) {
		this.attendance_date = attendance_date;
	}

	public int getAttendance_count() {
		return attendance_count;
	}

	public void setAttendance_count(int attendance_count) {
		this.attendance_count = attendance_count;
	}

	public int getCount() {
		return count;
	}

	public void setCount(int count) {
		this.count = count;
	}

	public String getMem_id() {
		return mem_id;
	}

	public void setMem_id(String mem_id) {
		this.mem_id = mem_id;
	}

	public String getDays_check() {
		return days_check;
	}

	public void setDays_check(String days_check) {
		this.days_check = days_check;
	}

	public String getDays_date() {
		return days_date;
	}

	public void setDays_date(String days_date) {
		this.days_date = days_date;
	}

}
